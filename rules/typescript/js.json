{
  "member-ordering": false,
  "no-import-side-effect": false,
  "no-magic-numbers": {"severity": "warn", "options": [-1, 0, 1, 10]},
  "no-parameter-reassignment": true,
  "no-reference": true,
  "only-arrow-functions": [
    true,
    "allow-declarations",
    "allow-named-functions"
  ],
  "prefer-for-of": true,
  "promise-function-async": true
}
